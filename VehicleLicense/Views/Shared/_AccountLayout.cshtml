<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - VehicleLicense</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/VehicleLicense.styles.css" asp-append-version="true" />


    <link rel="shortcut icon" type="image/x-icon" href="~/license/assets/images/favicon.png" />
    <link href="~/license/https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i&amp;display=swap"
          rel="stylesheet">
    <link href="~/license/https://fonts.googleapis.com/css?family=Rubik:300,300i,400,400i,500,500i,700,700i,900,900i&amp;display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/license/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/license/assets/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/license/assets/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/license/assets/css/animate.min.css">
    <link rel="stylesheet" href="~/license/assets/css/jquery-ui.css">
    <link rel="stylesheet" href="~/license/assets/css/slick.css">
    <link rel="stylesheet" href="~/license/assets/css/chosen.min.css">
    <link rel="stylesheet" href="~/license/assets/css/pe-icon-7-stroke.css">
    <link rel="stylesheet" href="~/license/assets/css/magnific-popup.min.css">
    <link rel="stylesheet" href="~/license/assets/css/lightbox.min.css">
    <link rel="stylesheet" href="~/license/assets/js/fancybox/source/jquery.fancybox.css">
    <link rel="stylesheet" href="~/license/assets/css/jquery.scrollbar.min.css">
    <link rel="stylesheet" href="~/license/assets/css/mobile-menu.css">
    <link rel="stylesheet" href="~/license/assets/fonts/flaticon/flaticon.css">
    <link rel="stylesheet" href="~/license/assets/css/style.css">


</head>
<body class="home">
    <header class="header style7">
        <div class="header-nav-container">
            <div class="container">
                <div class="header-nav-wapper main-menu-wapper">
                    <div class="header-nav">
                        <div class="container-wapper">
                            <ul class="tanajil-clone-mobile-menu tanajil-nav main-menu " id="menu-main-menu">
                                <li class="menu-item float-end">
                                    <a href="~/Home/Index" class="tanajil-menu-item-title" title="Home">Home</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <div class="header-device-mobile">
        <div class="wapper">
            <div class="item mobile-logo">
                <div class="logo">
                    <a href="#">
                        <img src="~/license/assets/images/logo.png" alt="img">
                    </a>
                </div>
            </div>
            <div class="item item mobile-search-box has-sub">
                <a href="#">
                    <span class="icon">
                        <i class="fa fa-search" aria-hidden="true"></i>
                    </span>
                </a>
                <div class="block-sub">
                    <a href="#" class="close">
                        <i class="fa fa-times" aria-hidden="true"></i>
                    </a>
                    <div class="header-searchform-box">
                        <form class="header-searchform">
                            <div class="searchform-wrap">
                                <input type="text" class="search-input" placeholder="Enter keywords to search...">
                                <input type="submit" class="submit button" value="Search">
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <div class="item mobile-settings-box has-sub">
                <a href="#">
                    <span class="icon">
                        <i class="fa fa-cog" aria-hidden="true"></i>
                    </span>
                </a>
                <div class="block-sub">
                    <a href="#" class="close">
                        <i class="fa fa-times" aria-hidden="true"></i>
                    </a>
                    <div class="block-sub-item">
                        <h5 class="block-item-title">Currency</h5>
                        <form class="currency-form tanajil-language">
                            <ul class="tanajil-language-wrap">
                                <li class="active">
                                    <a href="#">
                                        <span>
                                            English (USD)
                                        </span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <span>
                                            French (EUR)
                                        </span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <span>
                                            Japanese (JPY)
                                        </span>
                                    </a>
                                </li>
                            </ul>
                        </form>
                    </div>
                </div>
            </div>
            <div class="item menu-bar">
                <a class=" mobile-navigation  menu-toggle" href="#">
                    <span></span>
                    <span></span>
                    <span></span>
                </a>
            </div>
        </div>
    </div>
    @RenderBody()
    <script src="~/license/assets/js/jquery-1.12.4.min.js"></script>
    <script src="~/license/assets/js/jquery.plugin-countdown.min.js"></script>
    <script src="~/license/assets/js/jquery-countdown.min.js"></script>
    <script src="~/license/assets/js/bootstrap.min.js"></script>
    <script src="~/license/assets/js/owl.carousel.min.js"></script>
    <script src="~/license/assets/js/magnific-popup.min.js"></script>
    <script src="~/license/assets/js/isotope.min.js"></script>
    <script src="~/license/assets/js/jquery.scrollbar.min.js"></script>
    <script src="~/license/assets/js/jquery-ui.min.js"></script>
    <script src="~/license/assets/js/mobile-menu.js"></script>
    <script src="~/license/assets/js/chosen.min.js"></script>
    <script src="~/license/assets/js/slick.js"></script>
    <script src="~/license/assets/js/jquery.elevateZoom.min.js"></script>
    <script src="~/license/assets/js/jquery.actual.min.js"></script>
    <script src="~/license/assets/js/fancybox/source/jquery.fancybox.js"></script>
    <script src="~/license/assets/js/lightbox.min.js"></script>
    <script src="~/license/assets/js/owl.thumbs.min.js"></script>
    <script src="~/license/assets/js/jquery.scrollbar.min.js"></script>
    <script src='~/license/https://maps.googleapis.com/maps/api/js?key=AIzaSyC3nDHy1dARR-Pa_2jjPCjvsOR4bcILYsM'></script>
    <script src="~/license/assets/js/frontend-plugin.js"></script>

    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
    @RenderSection("script", required: false)

</body>
</html>
